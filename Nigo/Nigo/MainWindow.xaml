                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Button.Template>
            </Button>
            <Button Margin="28,0,0,0" x:Name="BtnInit" Width="54" Content="{wpf:Gettext INIT}"  Click="BtnInit_OnClick" IsEnabled="{Binding InitEnabled}">
                <Button.Template>
                    <ControlTemplate TargetType="{x:Type Button}">
