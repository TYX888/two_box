                                   <!--<TextBlock.Effect>
                                    <DropShadowEffect ShadowDepth="0" BlurRadius="15" Color="#00B050"/>
                                    </TextBlock.Effect>-->

                                </TextBlock>
                            </Grid>
                        </Label>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsEnabled"  Value="false">
                                <Setter  Property="Opacity" Value="0.3" ></Setter>
                            </Trigger>
                            <Trigger Property="IsMouseOver"  Value="true">
                                <Setter Property="Source" TargetName="img" Value="View/Resources/BtnMoveOn/STARTLOT.png"/>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Button.Template>
            </Button>
            <Button Margin="12,0,0,0" x:Name="BtnEndLot" Width="70" Content="{wpf:Gettext END LOT}"  Click="BtnEndLot_OnClick" IsEnabled="{Binding EndLotEnabled}">
                <Button.Template>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Label x:Name="radioGrid" Margin="-8,0,0,0">
                            <Grid Margin="0,15,0,0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="54" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="54"></RowDefinition>
                                    <RowDefinition Height="12"></RowDefinition>
                                    <RowDefinition Height="12"></RowDefinition>
                                </Grid.RowDefinitions>
                                <Image x:Name="img" Source="View/Resources/ENDLOT.png" HorizontalAlignment="Center">
                                </Image>
                                <TextBlock Grid.Row="2" x:Name="radioContent" VerticalAlignment="Center" Foreground="#23cefd" FontSize="12" 
                                                Text="{TemplateBinding Content}" HorizontalAlignment="Center" Block.TextAlignment="Center">
                                   <!--<TextBlock.Effect>
                                    <DropShadowEffect ShadowDepth="0" BlurRadius="15" Color="#00B050"/>
                                    </TextBlock.Effect>-->

                                </TextBlock>
                            </Grid>
                        </Label>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsEnabled"  Value="false">
                                <Setter  Property="Opacity" Value="0.3" ></Setter>
                            </Trigger>
                            <Trigger Property="IsMouseOver"  Value="true">
                                <Setter Property="Source" TargetName="img" Value="View/Resources/BtnMoveOn/ENDLOT.png"/>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Button.Template>
            </Button>

            <Button Margin="64,0,0,0" x:Name="BtnStart" Width="54" Content="{wpf:Gettext START}" Click="BtnStart_OnClick" IsEnabled="{Binding StartEnabled}">
                <Button.Template>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Label x:Name="radioGrid">
                            <Grid Margin="-7,15,0,0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="54" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="54"></RowDefinition>
                                    <RowDefinition Height="12"></RowDefinition>
                                    <RowDefinition Height="12"></RowDefinition>
                                </Grid.RowDefinitions>
                                <Image x:Name="img" Source="View/Resources/START.png" HorizontalAlignment="Center">
                                </Image>
                                <TextBlock Grid.Row="2" x:Name="radioContent" VerticalAlignment="Center" Foreground="#23cefd" FontSize="12" 
                                                Text="{TemplateBinding Content}" HorizontalAlignment="Center" Block.TextAlignment="Center">
