                            <Grid Margin="0,15,0,0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="54"></RowDefinition>
                                    <RowDefinition Height="12"></RowDefinition>
                                    <RowDefinition Height="12"></RowDefinition>
                                </Grid.RowDefinitions>
                                <Image x:Name="img" Source="View/Resources/STARTLOT.png" HorizontalAlignment="Center" Width="54">
                                </Image>
                                <TextBlock Grid.Row="2" x:Name="radioContent" VerticalAlignment="Center" Foreground="#23cefd" FontSize="12"  Width="66"
