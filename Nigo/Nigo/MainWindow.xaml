                </Button.Template>
            </Button>
            <Button Margin="28,0,0,0" x:Name="BtnEnd" Width="54"  Content="{wpf:Gettext END}"   Click="BtnEnd_OnClick" IsEnabled="{Binding EndEnabled}">
