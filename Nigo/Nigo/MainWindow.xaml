                                <TextBlock Grid.Row="2" x:Name="radioContent" VerticalAlignment="Center" Foreground="#23cefd" FontSize="12" 
                                                Text="{TemplateBinding Content}" HorizontalAlignment="Center" Block.TextAlignment="Center">
