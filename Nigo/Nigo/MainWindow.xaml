                    </ControlTemplate>
                </Button.Template>
            </Button>
            <!--<Button Margin="28,0,0,0" x:Name="BtnAbort" Width="54" Content="ABORT"  Click="BtnAbort_OnClick">
                <Button.Template>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Label x:Name="radioGrid">
                            <Grid Margin="-7,15,0,0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="54" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="54"></RowDefinition>
                                    <RowDefinition Height="12"></RowDefinition>
                                    <RowDefinition Height="12"></RowDefinition>
                                </Grid.RowDefinitions>
