                            <Grid Margin="-7,15,0,0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="54" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="54"></RowDefinition>
                                    <RowDefinition Height="12"></RowDefinition>
                                    <RowDefinition Height="12"></RowDefinition>
                                </Grid.RowDefinitions>
                                <Image x:Name="img" Source="View/Resources/INIT.png" HorizontalAlignment="Center">
                                </Image>
                                <TextBlock Grid.Row="2" x:Name="radioContent" VerticalAlignment="Center" Foreground="#23cefd" FontSize="12" 
