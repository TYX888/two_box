<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:wpf="clr-namespace:NGettext.Wpf;assembly=NGettext.Wpf"
        x:Name="Window" x:Class="Nigo.MainWindow"
        mc:Ignorable="d"  WindowChrome.WindowChrome="{DynamicResource WindowChromeKey}"
        Title="MainWindow" Height="1050" Width="1920"  MaxHeight="1050" MaxWidth="1920"
    WindowStartupLocation="CenterScreen" Closing="Window_Closing" WindowStyle="None" Loaded="Window_Loaded" >

    <Window.Resources>
        <WindowChrome x:Key="WindowChromeKey">
            <WindowChrome.ResizeBorderThickness>
                <Thickness>0</Thickness>
            </WindowChrome.ResizeBorderThickness>
        </WindowChrome>
    </Window.Resources>
    <Grid >
        <Grid.Background>
            <ImageBrush ImageSource="View/Resources/bg.png"/>
        </Grid.Background>
        <Grid.RowDefinitions>
            <!--<RowDefinition Height="30"/>-->
            <RowDefinition Height="74"/>
            <RowDefinition Height="867"/>
            <RowDefinition Height="109"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="230"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Border Grid.Row="0" Grid.Column="1" Grid.ColumnSpan="3"  BorderThickness="0,0,0,2" Margin="3"  Style="{StaticResource BlueBorder}" Visibility="Collapsed">
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                <Menu  Name="menu1"   Foreground="DeepSkyBlue" Background="Black"  FontSize="12" Margin="20,0,20,0" Width="900" HorizontalAlignment="Left">
                    <MenuItem Header="MAIN"   HorizontalContentAlignment="Center" Width="150">
                        <MenuItem Header="User control" Click="User_control" />
                        <MenuItem Header="-----" />
                    </MenuItem>
                    <MenuItem Header="DATA" Width="150"/>
                    <MenuItem Header="REMOTE" Width="150"/>
                    <MenuItem Header="LANGUAGE" Width="150"/>
                    <MenuItem Header="ABOUT" Width="150"/>
                </Menu>
            </StackPanel>
        </Border>

        <Grid  Grid.ColumnSpan="2">
            <Grid.Background>
                <ImageBrush ImageSource="/WpfStyleApi;component/Resources/Auto/Bg/AppBar.png"/>
            </Grid.Background>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"></ColumnDefinition>
                <ColumnDefinition Width="262"></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <StackPanel Orientation="Horizontal" Grid.Column="0" Grid.ColumnSpan="2">
                <Image Margin="20,0,0,0"  Source="/WpfStyleApi;component/Resources/Auto/AppBar/GHJ_logo.png" Width="186" Height="48"></Image>

                <StackPanel Orientation="Horizontal" Margin="55,0,0,0"  Width="800">
                    <Image Source="/WpfStyleApi;component/Resources/Auto/AppBar/left.png" Height="30" Width="35" Margin="0,0,0,0"></Image>
                    <TextBlock Margin="10,5,10,0" FontSize="34" Foreground="#4af0f7" VerticalAlignment="Center" TextDecorations="{x:Null}" FontFamily="/WpfStyleApi;component/Resources/Font/#Unisono Quickpath"> NVS-360</TextBlock>
                    <Image Source="/WpfStyleApi;component/Resources/Auto/AppBar/right.png" Height="30" Width="35" Margin="0,0,0,0"></Image>
                    <TextBlock Margin="10,5,0,0" FontSize="34" Foreground="#4af0f7" VerticalAlignment="Center" TextDecorations="{x:Null}" FontFamily="/WpfStyleApi;component/Resources/Font/#Unisono Quickpath"> AVI TESTER SYSTEM</TextBlock>
                </StackPanel>
                <Label  VerticalContentAlignment="Center" Foreground="#4af0f7"  HorizontalContentAlignment="Right">
                </Label>
                <Label Content="{wpf:Gettext OPERATOR:}" Foreground="#23cefd"  FontSize="16" Height="40" Width="110" Margin="123,0,0,0" VerticalContentAlignment="Center"></Label>
                <Label x:Name="lblUserName" Margin="0,0,28,0" Content="STMOP-20060001" Foreground="White" FontSize="16" Height="40" Width="160" VerticalContentAlignment="Center"></Label>

                <Border Visibility="Collapsed" Height="32" Width="1" Style="{StaticResource BlueBorder}" CornerRadius="0" Background="#23cefd"></Border>
                <Button Visibility="Collapsed" Margin="12,0,12,0" Height="68" Width="112"  BorderBrush="#23cefd" BorderThickness="0" x:Name="BtnLogin" Click="BtnLogin_OnClick" 
                        Content="{wpf:Gettext LOGIN}" FontSize="14" Foreground="#23cefd" >
                    <Button.Template>
                        <ControlTemplate TargetType="Button">
                            <Border x:Name="border"  BorderThickness="0"  BorderBrush="#33CCFF"  CornerRadius="5"  Height="68" Width="112" >
                                <Border.Background>
                                    <ImageBrush ImageSource="/WpfStyleApi;component/Resources/Auto/AppBar/Login.png"/>
                                </Border.Background>
                                <Grid >
                                    <TextBlock  x:Name="content"   VerticalAlignment="Center" 
                                        Text="{TemplateBinding Content}" HorizontalAlignment="Center" Block.TextAlignment="Center"></TextBlock>
                                </Grid>
                            </Border>
                            <ControlTemplate.Triggers>
                                <!--//当控件获取焦点时要执行的内容-->
                                <Trigger Property="IsKeyboardFocused"  Value="true"></Trigger>
                                <!--//当鼠标放上去时要设置的内容-->
                                <Trigger Property="IsMouseOver"  Value="true" >
                                    <Setter Property="Background" TargetName="border" >
                                        <Setter.Value>
                                            <ImageBrush ImageSource="/WpfStyleApi;component/Resources/Auto/AppBar/LOGIN_Mouse.png"/>
                                        </Setter.Value>
                                    </Setter>
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Button.Template>
                </Button>
                <Border Visibility="Collapsed" Height="32" Width="1" Style="{StaticResource BlueBorder}" CornerRadius="0" Background="#23cefd" Margin="0,0,0,0"></Border>

                <Button Visibility="Collapsed" x:Name="ButtonUserManager" Margin="12,0,12,0"  Height="68" Width="112"  Content="{wpf:Gettext User Mgr}"  FontSize="14" Click="BtnDebug" BorderThickness="0" Foreground="Red" HorizontalAlignment="Left">
                    <Button.Template>
                        <ControlTemplate TargetType="Button">
                            <Border x:Name="border"  BorderThickness="0"  BorderBrush="#33CCFF"  CornerRadius="5"  Height="68" Width="112" >
                                <Border.Background>
                                    <ImageBrush ImageSource="/WpfStyleApi;component/Resources/Auto/AppBar/Login.png"/>
                                </Border.Background>
                                <Grid >
                                    <TextBlock  x:Name="content"   VerticalAlignment="Center" 
                                        Text="{TemplateBinding Content}" HorizontalAlignment="Center" Block.TextAlignment="Center"></TextBlock>
                                </Grid>
                            </Border>
                            <ControlTemplate.Triggers>
                                <Trigger Property="IsKeyboardFocused"  Value="true"></Trigger>
                                <Trigger Property="IsMouseOver"  Value="true" >
                                    <Setter Property="Background" TargetName="border" >
                                        <Setter.Value>
                                            <ImageBrush ImageSource="/WpfStyleApi;component/Resources/Auto/AppBar/LOGIN_Mouse.png"/>
                                        </Setter.Value>
                                    </Setter>
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Button.Template>
                </Button>

            </StackPanel>

            <Grid Grid.Column="1">
                <Expander x:Name="MaxMinExpander" Header=" " Margin="0,0,0,0" IsExpanded="False" FontSize="18" Foreground="#03cefd" Style="{DynamicResource HeaderExpanderStyle}" HorizontalAlignment="Right"  Height="74" >
                    <StackPanel  Orientation="Horizontal" Background="#1eb0fc" Margin="-1,0,0,0">
                        <Button Height="16" Width="16"  Margin="96,0,0,0" x:Name="BtnMin"  BorderBrush="#23cefd" BorderThickness="1"  Click="BtnMin_Click">
                            <Button.Background>
                                <ImageBrush ImageSource="/WpfStyleApi;component/Resources/Auto/AppBar/min.png"/>
                            </Button.Background>
                        </Button>
                        <Button Height="16" Width="16"  Margin="32,0,0,0" x:Name="BtnMax"  BorderBrush="#23cefd" BorderThickness="1" Click="BtnMax_Click">
                            <Button.Background>
                                <ImageBrush ImageSource="/WpfStyleApi;component/Resources/Auto/AppBar/max.png"/>
                            </Button.Background>
                        </Button>
                        <Button Height="16" Width="16"  Margin="32,0,32,0" x:Name="BtnClose"  BorderBrush="#23cefd" BorderThickness="1" Click="BtnClose_Click">
                            <Button.Background>
                                <ImageBrush ImageSource="/WpfStyleApi;component/Resources/Auto/AppBar/close.png"/>
                            </Button.Background>
                        </Button>
                    </StackPanel>
                </Expander>
            </Grid>
            <Button Content="test" Visibility="Collapsed" FontSize="16" Width="100" Height="50" Margin="1010,10,540,14" Click="ButtonBase_OnClick"></Button>
        </Grid>

        <Grid Grid.Row="1"  Grid.RowSpan="2" Grid.Column="0" >
            <Grid.Background>
                <ImageBrush ImageSource="View/Resources/MenuBar_bg.png"/>
            </Grid.Background>
            <StackPanel >
                <Expander Header="{wpf:Gettext MODE}" IsExpanded="True" Style="{StaticResource MenuExpanderStyle}"  Foreground="#23cefd" FontSize="18"  FontWeight="Bold">
                    <StackPanel Margin="0,-8,0,0">
                        <RadioButton Height="75" Visibility="{Binding Source={StaticResource login},Path=Level,Converter={StaticResource VisibleConvert},ConverterParameter='h1'}" x:Name="RadioAuto" Click="RadioAuto_Click" GroupName="DefRadio"  Content="{wpf:Gettext AUTO}" Style="{StaticResource DefRadio}">
                        </RadioButton>
                        <RadioButton Height="75" x:Name="RadioManual" Click="RadioManual_Click"  GroupName="DefRadio" Content="{wpf:Gettext MANUAL}"  Style="{StaticResource DefRadio}" Tag="&#58894;" IsChecked="True"></RadioButton>
                    </StackPanel>
                </Expander>
                <Expander Header="{wpf:Gettext MODULE}" IsExpanded="True"  Style="{StaticResource MenuExpanderStyle}"  Foreground="#23cefd" FontSize="18" FontWeight="Bold">
                    <StackPanel Margin="0,-8,0,0">
                        <RadioButton Height="75" x:Name="RadioVision" Click="RadioVision_OnClick" GroupName="OrangeRadio" Content="{wpf:Gettext VISION}" Style="{StaticResource DefRadio}"  Tag="&#59073;" IsChecked="true"></RadioButton>
                        <RadioButton Height="75" x:Name="RadioTester" Click="RadioTester_OnClick" GroupName="OrangeRadio" Content="{wpf:Gettext TESTER}" Style="{StaticResource DefRadio}"  Tag="&#xe692;" ></RadioButton>
                        <RadioButton Height="75" x:Name="RadioStimulus" Click="RadioStimulus_OnClick" GroupName="OrangeRadio" Content="{wpf:Gettext STIMULUS}" Style="{StaticResource DefRadio}"  Tag="&#58935;" Visibility="Collapsed"></RadioButton>
                    </StackPanel>
                </Expander>
                <Expander Header="{wpf:Gettext SETTING}" IsExpanded="True"  Style="{StaticResource MenuExpanderStyle}" Foreground="#23cefd" FontSize="18" FontWeight="Bold">
                    <StackPanel Margin="0,-8,0,0">
                        <RadioButton Height="75" x:Name="RadioSystem" Click="RadioSystem_Click"  GroupName="PaleVioletRedRadio" Content="{wpf:Gettext SYSTEM}" Style="{StaticResource DefRadio}"  Tag="&#58937;" ></RadioButton>
                        <RadioButton Height="75" x:Name="RadioConfig" Click="RadioCONFIG_Click" GroupName="PaleVioletRedRadio" Content="{wpf:Gettext CONFIG}" Style="{StaticResource DefRadio}"  Tag="&#58925;" ></RadioButton>
                        <RadioButton Height="75" x:Name="RadioCali" Click="RadioCALI_Click" GroupName="PaleVioletRedRadio" Content="{wpf:Gettext CALI}" Style="{StaticResource DefRadio}"  Tag="&#58912;"></RadioButton>
                    </StackPanel>
                </Expander>
                <Expander Visibility="Collapsed" Header="{wpf:Gettext DATA}" IsExpanded="True"  Style="{StaticResource MenuExpanderStyle}" Foreground="#23cefd" FontSize="18" FontWeight="Bold">
                    <StackPanel Margin="0,-8,0,0">
                        <RadioButton x:Name="RadioReport" Click="RadioREPORT_Click"  GroupName="PaleVioletRedRadio" Content="{wpf:Gettext REPORT}" Style="{StaticResource DefRadio}"  Tag="&#58904;"></RadioButton>

                    </StackPanel>
                </Expander>
            </StackPanel>
        </Grid>

        <Grid x:Name="GridGlobal"  Grid.Row="1" Grid.Column="1"  Grid.RowSpan="2" Margin="0,0,0,0"></Grid>

        <StackPanel x:Name="PanelRun" Grid.Row="2" Grid.Column="1" Orientation="Horizontal" Margin="30,-5,0,10">
