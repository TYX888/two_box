                                <Setter Property="Source" TargetName="img" Value="View/Resources/BtnMoveOn/END.png"/>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Button.Template>
            </Button>
            <Button Margin="28,0,0,0" x:Name="BtnReset" Width="54" Content="{wpf:Gettext RESET}"   Click="BtnReset_OnClick" IsEnabled="{Binding ResetEnabled}">
                <Button.Template>
                    <ControlTemplate TargetType="{x:Type Button}">
